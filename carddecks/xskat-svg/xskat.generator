<!-- Cards -->
EOF

# include external court images
generate_court_images();

# Generate all groups
generate_groups("heart", "heart");
generate_groups("diamond", "bell");
generate_groups("spade", "leaf");
generate_groups("club", "acorn");

# generate all cards: for each suit and each card name generate a card
my $leftborder = $off;
my $rightborder = $off + $figwidth;
my $centery = $height / 2 - 0.2;
for ($j = 0; $j < @suits; $j++) {
	print "<!-- $suits[$j] suit -->\n";
	for ($i = 0; $i < @cardnames; $i++) {
		my $cardid = "$cardnames[$i]_$suits[$j]";
		print <<EOF;
<g id="$cardid">
<use xlink:href="#card-$suits[$j]" />
<use x="$off" y="$off" xlink:href="#g-$cardid" />
<use x="$off" y="$off" xlink:href="#g-$cardid" transform="rotate(180,$centerx,$centery)" />
<use xlink:href="#card-shadow" />
</g>
EOF
	}
}

print <<EOF;
<!-- Card groups, for easy displaying -->
EOF

# generate all suits (not strictly needed, but nice to see the cards when
# loading the deck in an editor)
for ($j = 0; $j < @suits; $j++) {
	print "<g id=\"$suits[$j]-suit\">\n";
	for ($i = 0; $i < @cardnames; $i++) {
		my $cardid = "$cardnames[$i]_$suits[$j]";
		my $x = $width * $i;
		print <<EOF;
<use x="$x" xlink:href="#$cardid" />
EOF
	}
	print "</g>\n";
}


my $boff = 4;
my $b2off = 6;
my $bwidth = $width - 2 * $boff;
my $bheight = $height - 2 * $boff;
my $bradius = $cradius - $boff;
my $b2width = $width - 2 * $b2off;
my $b2height = $height - 2 * $b2off;
my $b2radius = $cradius - $b2off;
my $cx = $width / 2;
my $cy = $height / 2;

print <<EOF;
<!-- Misc stuff: Card backs, jolly jokers... -->
<g id="back">
<use xlink:href="#card-outline"/>
<rect width="$b2width" height="$b2height" x="$b2off" y="$b2off" rx="$b2radius" style="fill:#080;"/>
<rect width="$bwidth" x="$boff" y="$boff" rx="$bradius" height="$bheight" style="fill:none;stroke:#04b;stroke-width:1;"/>
<use x="$cx" y="$cy" style="fill:white;" xlink:href="#big-logo"/>
<use xlink:href="#back-shadow"/>
</g>
<g id="misc-cards">
<use x="0" xlink:href="#back"/>
</g>
</defs>
EOF

# generate suits for display
for ($j = 0; $j < @suits; $j++) {
	my $y = (0 + $height) * $j;
	print <<EOF;
<use y="$y" xlink:href="#$suits[$j]-suit"/>
EOF
}

my $y = $height * (@suits);
print <<EOF;
<use y="$y" xlink:href="#misc-cards"/>
</svg>
<!--
 vi: ts=4 sw=4 noautoindent
-->
EOF
