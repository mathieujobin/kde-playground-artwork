#!/usr/bin/perl
#
# This script generates an SVG card deck suitable for KDE
# card games.
# Currently developed to generate a deck for lskat from a public domain image.
#

use MIME::Base64;

my $year = 2009;
my $author = "Luciano Montanaro <mikelima@cirulla.net>";

# Width and height of the card board
my $boardwidth = 1024;
my $boardheight = 804;

# Width and height of each card
my $width = 144;
my $height = 224;

# corner radius
my $cradius = 9; 

# offset from card border to pattern border
my $off = 6;

my $xoff1 = 2 * $off;
my $yoff = $off * 5;
my $ioff = $off * 2.5;
my $xoff2 = $width - $xoff1;

# width and height of card figure
my $figwidth = $width - 2 * $off;
my $figheight = $height - 2 * $off;
my $figradius = $cradius - 2;

my $centerx = $width / 2;
my $centery = $height / 2 - 1;

my @suits = ("heart", "diamond", "spade", "club");

# A Skat deck only has cards down to 7. The Ace is really the Queen, King is
# itself, queen is high knave, jack is low knave. I guess.
my @cardnames = ("1", "king", "queen", "jack", "10", "9", "8", "7", "6", "5", "4", "3", "2");

my $viewwidth =  @cardnames * $width;
my $viewheight = (1 + @suits) * $height;

my $ratiox = 1;
my $ratioy = 1;

my $suitsize = 16;
my $spacing = 7.4;

my @column = (
	$centerx - 2 * ($suitsize + 3) - $off, 
	$centerx - ($suitsize - 2) - $off, 
	$centerx - $off, 
	$centerx + ($suitsize - 2)- $off,
	$centerx + 2 * ($suitsize + 3) - $off);
my @row = (
	2 + $off + $spacing, 
	2 + $off + 2 * $spacing, 
	2 + $off + 3 * $spacing,  
	2 + $off + 4 * $spacing, 
	2 + $off + 5 * $spacing,
	2 + $off + 6 * $spacing,
	2 + $off + 7 * $spacing,
	2 + $off + 8 * $spacing,
	2 + $off + 9 * $spacing,
	2 + $off + 10 * $spacing,
	2 + $off + 11 * $spacing,
);

# Rows for "branched" suits.
my $bspacing = 16;

my @brow = (
	$off + $bspacing, 
	$off + 2 * $bspacing, 
	$off + 3 * $bspacing,  
	$off + 4 * $bspacing, 
	$off + 5 * $bspacing, 
	$off + $figwidth / 2, 
);

sub generate_court_images()
{
	for ($i = 0; $i < 4; $i++) {
		for ($j = 1; $j < 4; $j++) {
			my $pngname = 1 + $j * 4 + $i;
			my $offsetx = $j * $width;
			my $offsety = $i * $height;
			my $cardid = "$cardnames[$j]_$suits[$i]";
			open(CARD, "<images/$cardid.png");
			binmode(CARD);
			my @card_data = <CARD>;
			my $card_encoded = encode_base64(join("", @card_data));
			print <<EOF;
<g id="img_$cardid" transform="scale(1) translate(-64,-98)" >
<image width="113" height="93" xlink:href="data:image/png;base64,$card_encoded" />
</g>
EOF
		}
	}
}

