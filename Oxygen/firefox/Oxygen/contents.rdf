<?xml version="1.0"?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- List all the skins being supplied by this theme -->
  <RDF:Seq about="urn:mozilla:skin:root">
    <RDF:li resource="urn:mozilla:skin:Oxygen"/>
  </RDF:Seq>
  
  <RDF:Description about="urn:mozilla:skin:Oxygen" 
        chrome:displayName="Oxygen"
        chrome:accessKey="N"
        chrome:author="The Oxygen Artists"
        chrome:authorURL="http://www.oxygen-icons.org"
        chrome:description="Oxygen Icons"
        chrome:name="Oxygen"
        chrome:image="preview.png" >
   <chrome:packages>
      <RDF:Seq about="urn:mozilla:skin:Oxygen:packages">
        <RDF:li resource="urn:mozilla:skin:Oxygen:browser"/>
        <RDF:li resource="urn:mozilla:skin:Oxygen:communicator"/>
        <RDF:li resource="urn:mozilla:skin:Oxygen:global"/>
        <RDF:li resource="urn:mozilla:skin:Oxygen:mozapps"/>
        <RDF:li resource="urn:mozilla:skin:Oxygen:help"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description>

  <!-- Version Information.  State that we work only with major version 1 of this package. -->
  <RDF:Description chrome:skinVersion="1.5" about="urn:mozilla:skin:Oxygen:browser"/>
  <RDF:Description chrome:skinVersion="1.5" about="urn:mozilla:skin:Oxygen:communicator"/>
  <RDF:Description chrome:skinVersion="1.5" about="urn:mozilla:skin:Oxygen:global"/>
  <RDF:Description chrome:skinVersion="1.5" about="urn:mozilla:skin:Oxygen:mozapps"/>
  <RDF:Description chrome:skinVersion="1.5" about="urn:mozilla:skin:Oxygen:help"/>
</RDF:RDF>
