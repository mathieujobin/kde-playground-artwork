<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>oxygen-firefox-theme@oxygen-icons.org</em:id>
    <em:version>1.0</em:version>

    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <!-- Firefox's UUID -->
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>3.9</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Front End MetaData -->
    <em:name>Oxygen</em:name>
    <em:description>Oxygen for Mozilla Firefox</em:description>
    <em:creator>Harald Sitter</em:creator>
    <em:contributor>The Oxygen Artwork Team</em:contributor>
    <em:contributor>David Vignoni</em:contributor>
    <em:contributor>Nuno Pinheiro</em:contributor>
    <em:contributor>Kenneth Wimer</em:contributor>
    <em:contributor>Riccardo Iaconelli</em:contributor>
    <em:contributor>Johann Ollivier Lapeyre</em:contributor>
    <em:contributor>David Miller</em:contributor>
    <em:homepageURL>http://www.oxygen-icons.org</em:homepageURL>
    
    <!-- Front End Integration Hooks (used by Theme Manager)-->
    <em:internalName>oxygen</em:internalName>
  </Description>
  
</RDF>
