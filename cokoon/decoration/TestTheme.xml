		</source>
	</object>


	<object id="Button.close.normal.*.*">
<!--		<expression id="ButtonWidth" value="20"/>
		<expression id="ButtonHeight" value="18"/>-->

		<source id="iconsrc" type="svg" file="TestTheme/icon_close.svg"/>
		
		<tile id="buttonicon" mode_hor="stretched" mode_vert="stretched" source_id="iconsrc" left="0" top="0" width="10" height="10"/>

		<expression id="iconMargin" value="4"/>

		<layout id="icon">
			<column type="fixed" fixed_size=".o.iconMargin"/>
			<column type="expanding"/>
			<column type="fixed" fixed_size=".o.iconMargin"/>
			<row type="fixed" fixed_size=".o.iconMargin">
				<cell type="empty"/>
				<cell type="empty"/>
				<cell type="empty"/>
			</row>
			<row type="expanding">
				<cell type="empty"/>
				<cell type="tile" tile_id="buttonicon"/>
				<cell type="empty"/>
			</row>
			<row type="fixed" fixed_size=".o.iconMargin">
				<cell type="empty"/>
				<cell type="empty"/>
				<cell type="empty"/>
			</row>
		</layout>

		<layout id="icon_sunken">
			<column type="fixed" fixed_size=".o.iconMargin"/>
			<column type="expanding"/>
			<column type="fixed" fixed_size=".o.iconMargin"/>
			<row type="fixed" fixed_size=".o.iconMargin+1">
				<cell type="empty"/>
				<cell type="empty"/>
				<cell type="empty"/>
			</row>
			<row type="expanding">
				<cell type="empty"/>
				<cell type="tile" tile_id="buttonicon"/>
				<cell type="empty"/>
			</row>
			<row type="fixed" fixed_size=".o.iconMargin-1">
				<cell type="empty"/>
				<cell type="empty"/>
				<cell type="empty"/>
			</row>
		</layout>

		<paint>
			<layer layout_object="button_normal" layout_id="button"/>
			<layer layout_id="icon"/>
		</paint>
	</object>
	<object id="Button.close.prelight.*.*" inherit="..normal.active.normal">
