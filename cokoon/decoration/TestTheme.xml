<?xml version="1.0" encoding="UTF-8"?>
<cokoon_theme name="TestTheme" version="0.1pre">
	<object id="Settings">
		<expression id="RespectBorderSizeFactor" value="1"/>
		<expression id="CustomButtonSize" value="0"/>
		<expression id="MenuDoubleClickClose" value="0"/>
		<expression id="WindowTitleAlignment" value="0"/>
	</object>

	<object id="Decoration.active.normal.*.*">
		<source id="decorationsrc" type="svg" file="testdecoration.svg">
			<replace_style_attr element_id="rect2788" attr="fill" value="ColorTitleBar"/>
		</source>
		<source id="masksrc" type="bitmap" file="TestTheme/windowmask.png"/>

		<expression id="zoom" value="1"/>

		<expression id="srcwidth" value="50"/>
		<expression id="srcheight" value="50"/>
		<expression id="edgesize" value="6"/>
		<expression id="titleheight" value="40-6"/>

		<expression id="bsize" value="BorderSizeFactor*6"/>

		<expression id="BorderLeft" value=".bsize"/>
		<expression id="BorderRight" value=".bsize"/>
		<expression id="BorderBottom" value=".bsize"/>

		<expression id="TitleHeight" value="WindowTitleHeight"/>
		<expression id="TitleEdgeBottom" value="1"/>

		<expression id="TitleEdgeTop" value=".edgesize"/>
		<expression id="TitleEdgeLeft" value=".bsize"/>
		<expression id="TitleEdgeRight" value=".bsize"/>

		<expression id="TitleBorders" value="5"/>

		<expression id="ButtonSpacing" value="1"/>
		<expression id="ExplicitButtonSpacer" value="3"/>

		<expression id="WindowTitleColor" value="ColorFont"/>

		<tile id="WindowMaskTopLeft" source_id="masksrc" mode_hor="normal" mode_vert="normal" left="0" top="0" width="2" height="2"/>
		<tile id="WindowMaskTopRight" source_id="masksrc" mode_hor="normal" mode_vert="normal" left="1" top="0" width="2" height="2"/>
		<tile id="WindowMaskBottomLeft" source_id="masksrc" mode_hor="normal" mode_vert="normal" left="0" top="0" width="1" height="1"/>
		<tile id="WindowMaskBottomRight" source_id="masksrc" mode_hor="normal" mode_vert="normal" left="0" top="0" width="1" height="1"/>

		<tile id="topleft" mode_hor="stretched" mode_vert="stretched" source_id="decorationsrc" left="0" top="0" width=".o.edgesize" height=".o.edgesize"/>
		<tile id="top" mode_hor="tiled" mode_vert="stretched" source_id="decorationsrc" left="10" top="0" width="2" height=".o.edgesize"/>
		<tile id="topright" mode_hor="stretched" mode_vert="stretched" source_id="decorationsrc" left=".o.srcwidth-.o.edgesize" top="0" width=".o.edgesize" height=".o.edgesize"/>

		<tile id="titleleft" mode_hor="stretched" mode_vert="stretched" source_id="decorationsrc" left="0" top=".o.edgesize" width=".o.edgesize" height=".o.titleheight"/>
		<tile id="title" mode_hor="tiled" mode_vert="stretched" source_id="decorationsrc" left="10" top=".o.edgesize" width="2" height=".o.titleheight"/>
		<tile id="titleright" mode_hor="stretched" mode_vert="stretched" source_id="decorationsrc" left=".o.srcwidth-.o.edgesize" top=".o.edgesize" width=".o.edgesize" height=".o.titleheight"/>

		<tile id="titlebottom" mode_hor="tiled" mode_vert="stretched" source_id="decorationsrc" left="10" top=".o.edgesize+.o.titleheight" width="2" height="1"/>

		<tile id="left" mode_hor="stretched" mode_vert="tiled" source_id="decorationsrc" left="0" top=".o.srcheight-.o.edgesize-1" width=".o.edgesize" height="2"/>
		<tile id="right" mode_hor="stretched" mode_vert="tiled" source_id="decorationsrc" left=".o.srcwidth-.o.edgesize" top=".o.srcheight-.o.edgesize-1" width=".o.edgesize" height="2"/>

		<tile id="bottomleft" mode_hor="stretched" mode_vert="stretched" source_object="" source_id="decorationsrc" left="0" top=".o.srcheight-.o.edgesize" width=".o.edgesize" height=".o.edgesize"/>
		<tile id="bottom" mode_hor="tiled" mode_vert="stretched" source_id="decorationsrc" left="10" top=".o.srcheight-.o.edgesize" width="2" height=".o.edgesize"/>
		<tile id="bottomright" mode_hor="stretched" mode_vert="stretched" source_id="decorationsrc" left=".o.srcwidth-.o.edgesize" top=".o.srcheight-.o.edgesize" width=".o.edgesize" height=".o.edgesize"/>
		<tile id="center" mode_hor="tiled" mode_vert="stretched" source_id="decorationsrc" left="10" top=".o.edgesize" width="2" height=".o.srcheight-2*.o.edgesize"/>
		<layout id="bglayout">
			<column type="fixed" fixed_size=".o.BorderLeft"/>
			<column type="expanding"/>
			<column type="fixed" fixed_size=".o.BorderRight"/>
			<row type="fixed" fixed_size=".o.TitleEdgeTop">
				<cell type="tile" tile_id="topleft"/>
				<cell type="tile" tile_id="top"/>
				<cell type="tile" tile_id="topright"/>
			</row>
			<row type="fixed" fixed_size=".o.TitleHeight">
				<cell type="tile" tile_id="titleleft"/>
				<cell type="tile" tile_id="title"/>
				<cell type="tile" tile_id="titleright"/>
			</row>
			<row type="fixed" fixed_size=".o.TitleEdgeBottom">
				<cell type="tile" tile_id="left"/>
				<cell type="tile" tile_id="titlebottom"/>
				<cell type="tile" tile_id="right"/>
			</row>
			<row type="expanding">
				<cell type="tile" tile_id="left"/>
				<cell type="empty"/>
				<cell type="tile" tile_id="right"/>
			</row>
			<row type="fixed" fixed_size=".o.BorderBottom">
				<cell type="tile" tile_id="bottomleft"/>
				<cell type="tile" tile_id="bottom"/>
				<cell type="tile" tile_id="bottomright"/>
			</row>
		</layout>
		<layout id="title">
			<column type="fixed" fixed_size=".o.TitleEdgeLeft+ButtonGroupLeftWidth+.o.TitleBorders"/>
			<column type="expanding"/>
			<column type="fixed" fixed_size=".o.TitleBorders+ButtonGroupRightWidth+.o.TitleEdgeRight"/>
			<row type="fixed" fixed_size=".o.TitleEdgeTop">
				<cell type="empty"/>
				<cell type="empty"/>
				<cell type="empty"/>
			</row>
			<row type="fixed" fixed_size=".o.TitleHeight">
				<cell type="empty"/>
				<cell type="special" special_id="WindowTitle"/>
				<cell type="empty"/>
			</row>
		</layout>
		<paint>
			<layer layout_id="bglayout"/>
			<layer layout_id="title"/>
		</paint>
	</object>

